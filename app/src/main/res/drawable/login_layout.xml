<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android" >
<!--设置背景-->
    <solid android:color="#FFFFFF" />
<!--设置边框-->
    <stroke
        android:width="0.5dp"
        android:color="@color/gray" />
<!--设置内边距-->
    <padding
        android:bottom="1dp"
        android:left="1dp"
        android:right="1dp"
        android:top="1dp" />
<!--设置圆角-->
    <corners android:radius="5dp" />

</shape>
